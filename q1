#include<iostream>
using namespace std;
int main()
{    int n,j=0,f=0,arr[100],s[100],x=-1,temp;
     cin>>n;
     temp=n-1;
     if(n>=1 && n<=10000)
     {    for(int i=2;i<temp;i++)
     		{  while(n%i==0)
     			{   n=n/i;
     				//cout<<n<<" ";
				    arr[j]=i;
					j++; 
     				f=1 ;				
				    if(n==0)
			     	 break;
		    	}
     			
     			
			 }
     	if(f==0)
     	cout<<"Not possible";
     	else
     	{
		  for(int i=0;i<j-1;i++) 
		  {  if((arr[i]*arr[i+1])<10)
		      {
			  arr[i+1]=arr[i]*arr[i+1];
		      arr[i]=0;
			  }
		   } 
		   for(int i=0;i<j;i++)
		   {   if(arr[i]!=0)
		      	{ s[++x]=arr[i];					
		   		}
	       }
	      for(int i=0;i<=x;i++)
	       {	for(int j=0;j<=(x-1)-i;j++)
	       		{ if(s[j]>s[j+1])
	       			{ int t;
					   t=s[j];
	       			s[j]=s[j+1];
	       			s[j+1]=t;
	       			
				    }
	       			
				}
	       	
		   }
		  for(int i=0;i<=x;i++)
		   cout<<s[i];
	       
	       
	     
      	}	
     	
	 }
	 else 
	 cout<<"Not possible";
	
	
	
return 0;	
}
